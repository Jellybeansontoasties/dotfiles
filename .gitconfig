[user]
	email = juliajeantrinidadagustin@gmail.com
	name = jellybeansontoasties

[core]
	editor = nano
	autocrlf = input
	filemode = true

[init]
	defaultBranch = main

[color]
	ui = auto

[alias]
	st = status
	co = checkout
	br = branch
	ci = commit
	cp = cherry-pick
	unstage = reset HEAD --
	last = log -1 HEAD
	visual = !code
	graph = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --all
	amend = commit --amend --no-edit
	undo = reset --soft HEAD~1
